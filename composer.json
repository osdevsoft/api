{
  "name": "osds/api",
  "description": "Easy API for Frameworks",
  "authors": [
    {
      "name": "Open Source Development Services",
      "email": "info@osds.com"
    }
  ],
  "version": "1.0.12",
  "require": {
    "doctrine/annotations": "^1.8",
    "nelmio/api-doc-bundle": "^3.6",
    "symfony/asset": "^5.0",
    "osds/ddd-common": "^1.0",
    "osds/auth": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1",
    "guzzlehttp/guzzle": "^6.3",
    "behat/behat": "^3.6"
  },

  "autoload": {
    "psr-4": {
      "Osds\\Api\\": "src/"
    },
    "autoload-dev": {
      "psr-4": {
        "Osds\\Api\\": "tests/"
      }
    }
  },

  "scripts": {
    "post-install-cmd": [
      "touch /tmp/qwer"
    ]
  },

  "repositories": {
    "osds-ddd-common": {
      "type": "vcs",
      "url": "https://github.com/osdevsoft/ddd-common"
    },
    "osds-auth": {
      "type": "vcs",
      "url": "https://github.com/osdevsoft/auth"
    }
  }

}
