{
  "name": "osds/api",
  "description": "Easy API for Laravel, Symfony Frameworks",
  "authors": [
    {
      "name": "Open Source Development Services",
      "email": "info@osds.com"
    }
  ],
  "minimum-stability": "dev",
  "version": "1.0a",
  "require": {
    "symfony/orm-pack": "^1.0",
    "elasticsearch/elasticsearch": "^6.7",
    "php-amqplib/rabbitmq-bundle": "^1.14",
    "illuminate/http": "^5.6",
    "doctrine/annotations": "~1.0",
    "symfony/monolog-bundle": "^3.3",
    "nelmio/api-doc-bundle": "^3.3",
    "symfony/asset": "^4.1",
    "ramsey/uuid": "^3.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1"
  },

  "autoload": {
    "psr-4": {
      "Osds\\Api\\": "src/"
    },
    "autoload-dev": {
      "psr-4": {
        "Osds\\Api\\": "tests/"
      }
    },

    "files": [
      "src/utils.php"
    ]
  },

  "scripts": {
    "post-install-cmd": [
      "touch /tmp/qwer"
    ]
  }

}
