{
  "name": "osds/api",
  "description": "Easy API for Laravel, Symfony Frameworks",
  "authors": [
    {
      "name": "Open Source Development Services",
      "email": "info@osds.com"
    }
  ],
  "minimum-stability": "dev",
  "version": "1.0a",
  "require": {
    "doctrine/annotations": "~1.0",
    "nelmio/api-doc-bundle": "^3.3",
    "symfony/asset": "^4.1",
    "lexik/jwt-authentication-bundle": "^2.6",
    "ramsey/uuid": "^3.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1",
    "guzzlehttp/guzzle": "^6.3",
    "behat/behat": "^3.5"
