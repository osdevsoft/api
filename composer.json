  "autoload": {
    "psr-4": {
      "Osds\\Api\\": "src/"
    },
    "autoload-dev": {
      "psr-4": {
        "Osds\\Api\\": "tests/"
      }
    }
  },

  "scripts": {
    "post-install-cmd": [
      "touch /tmp/qwer"
    ]
  }

}
